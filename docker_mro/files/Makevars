CXX14FLAGS=-w -m64 -DINT_64BITS -DARMA_64BIT_WORD -O2 -march=native -mtune=native -w -std=c++14 -m64 -fno-strict-aliasing -fomit-frame-pointer -funroll-loops -fexpensive-optimizations -fPIC
